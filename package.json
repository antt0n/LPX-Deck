{
  "name": "launchpadcore",
  "version": "0.2.0",
  "description": "Simple way to control your Novation Launchpad.",
  "main": "./lib/launchpadcore-min.js",
  "types": "./lib/launchpadcore.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "dev": "ts-node src/index",
    "demo": "ts-node demo/index",
    "build": "rollup --config",
    "clean": "rd /s /q lib>nul 2>&1|echo.>nul",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run format && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antt0n/Launchpad-Core"
  },
  "engines": {
    "node": "14.* || >=16.*"
  },
  "keywords": [
    "novation",
    "launchpad",
    "launchpad x",
    "launchpad pro",
    "launchpad mk2",
    "launchpad mk3",
    "launchpad mini",
    "launchpad core",
    "core",
    "novation launchpad"
  ],
  "author": {
    "name": "Antoine Seguin",
    "email": "pro@antoine-seguin.fr",
    "web": "https://antoine-seguin.fr"
  },
  "license": "ISC",
  "devDependencies": {
    "@ampproject/rollup-plugin-closure-compiler": "^0.27.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.2",
    "@types/node": "^17.0.39",
    "prettier": "^2.6.2",
    "rollup": "^2.75.5",
    "rollup-plugin-banner": "^0.2.1",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-filesize": "^9.1.2",
    "rollup-plugin-generate-package-json": "^3.2.0",
    "rollup-plugin-progress": "^1.1.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-visualizer": "^5.6.0",
    "ts-node": "^10.8.0",
    "typescript": "^4.7.2"
  },
  "dependencies": {
    "jzz": "^1.5.1"
  }
}
